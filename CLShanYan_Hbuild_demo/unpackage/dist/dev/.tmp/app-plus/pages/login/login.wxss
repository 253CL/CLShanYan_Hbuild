	    -ms-flex-pack: center;
	        justify-content: center;
	margin: 0 10rpx;
	border-radius: 10rpx;
	width: 100%;
}
.text_attr {
	width: 95%;
	margin-left: 30rpx;
	margin-top: 10rpx;
	margin-bottom: 10rpx;
	margin-right: 30rpx;
	word-wrap: break-word;
	word-break: break-all;
	font-size: 30rpx;
}
.button_attr {
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	margin-top: 30rpx;
	margin-left: 15rpx;
	margin-right: 15rpx;
	margin-bottom: 30rpx;
}
.tableTitle {
	position: relative;
	margin-top: 50rpx;
	margin-bottom: 50rpx;
	width: 100%;
	height: 1px;
	background-color: #d4d4d4;
	text-align: center;
	font-size: 16px;
	color: rgba(101, 101, 101, 1);
}
.midText {
	position: absolute;
	left: 50%;
	background-color: #f2f2f5;
	-webkit-transform: translateX(-50%) translateY(-50%);
	    -ms-transform: translateX(-50%) translateY(-50%);
	        transform: translateX(-50%) translateY(-50%);
